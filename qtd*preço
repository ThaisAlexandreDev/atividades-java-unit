import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    
	    Scanner input = new Scanner(System.in);
	    
	    System.out.print("Quantidade vendida: ");
	    int quantidadeDeProduto = input.nextInt();
	    
        System.out.print("\nPreço do Produto: ");
        float precoProduto = input.nextFloat();
    
        float valorTotal = quantidadeDeProduto*precoProduto;
    
		System.out.printf("\nO resultado é: %.1f" , valorTotal);
		input.close();
	}
}
